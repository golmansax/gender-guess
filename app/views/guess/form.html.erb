<h1>Enter your height and weight and let me guess your gender</h1>

<%= form_for :person, url: guess_parse_path do |form| %>
  <%= form.number_field :height, placeholder: 'Enter height (in inches)' %>
  <%= form.number_field :weight, placeholder: 'Enter weight (in pounds)' %>
  <%= form.submit 'Try me!' %>
<% end %>
